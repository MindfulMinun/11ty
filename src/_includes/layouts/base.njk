---
lang: en-US
---
<!doctype html>
<!--
    Made with love by MindfulMinun
    https://benjic.xyz
-->
<html lang="{{ lang }}">
<head>
    <meta charset="utf-8">
    <title>{{ title }}</title>
    <meta name="viewport" content="width=device-width">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="theme-color" content="#1e1e1e">

    <!-- OpenGraph & Twitter -->
    <meta name="og:title"       content="Notes">
    <meta name="og:description" content="I’m supposed to be studying. You too, probably.">
    <meta name="description"    content="I’m supposed to be studying. You too, probably.">
    <meta name="og:type"        content="website">
    <meta name="og:url"         content="https://notes.benjic.xyz/">

    <meta property="og:image" content="URL to image for thumbnail (square 350x350px)" />

    <!-- Begin Twitter tags -->
    <meta name="twitter:title" content="Your page title" />
    <meta name="twitter:url" content="Your page URL" />
    <meta name="twitter:card" content="Your page description" />
    <meta name="twitter:image" content="URL to image for thumbnail (square 350x350px)" />
    <!-- End Twitter tags -->

    <link rel="stylesheet" href="/static/styles/normalize.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Material+Icons|Roboto:100,100i,300,300i,400,400i,500,500i|Roboto+Mono:300,300i,400,400i&display=swap">
    <link rel="stylesheet" href="/static/styles/base.css">
</head>
<body class="flex flex--column">
    <main class="container flex-grow">
        {{ content | safe }}
    </main>
    {% footer %}
</body>
</html>
