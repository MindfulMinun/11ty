{
  "name": "11ty",
  "version": "0.1.0",
  "description": "A static site built with 11ty",
  "author": "MindfulMinun <importantmails4bc@gmail.com> (https://benjic.xyz)",
  "contributors": [],
  "homepage": "https://github.com/MindfulMinun/11ty",
  "repository": "github:MindfulMinun/11ty",
  "license": "MIT",
  "private": true,
  "_type": "module",
  "scripts": {
    "build::webp": "`for file in dist/static/images/*; do cwebp -q 80 \"$file\" -o \"${file%.*}.webp\"; done`",
    "build": "rm -rf ./dist && npx @11ty/eleventy && npm run build::webp",
    "watch": "rm -rf ./dist && npx @11ty/eleventy --serve --port=1337",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">=8.0.0 <11.0.0"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.0"
  }
}
